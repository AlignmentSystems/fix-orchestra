<!DOCTYPE html>
<html>
<body>
$fields:{field|<a href="$field.name$.html">$field.name$</a><br>}$
</body>
</html>
>>

messageStart(message) ::= <<
<!DOCTYPE html>
<html>
<title>Message $message.name$ Scenario $message.scenario$</title>
<head>
  <link rel="stylesheet" href="../fixstyle.css">
</head>
<body>
<h2>Message $message.name$ Scenario $message.scenario$</h2>
$if(message.extends)$extends scenario <a href="$message.name$-$message.extends$.html">$message.extends$</a><br>$endif$
$documentation(object=message)$
<br>
<table>
<tr>
<th>ID</th>
<th>MsgType</th>
<th>Flow</th>
</tr>
<tr><td>$message.id$</td><td>$message.msgType$</td><td>$message.flow$</td></tr>
</table>
$pedigree(object=message)$
<h3>Responses</h3>
<table>
<tr>
<th>Message</th>
<th>Scenario</th>
<th>When</th>
</tr>
>>

messageResponse(message, scenario, when) ::= <<
<tr><td>$message$</td><td>$scenario$</td><td>$when$</td></tr>
>>

messagePart2(message) ::= <<
</table>
<h3>Members</h3>
<table>
<tr>
<th>ID</th>
<th>Name</th>
<th>Presence</th>
</tr>
>>



messageEnd(message) ::= <<
</table>
</body>
</html>
>>

