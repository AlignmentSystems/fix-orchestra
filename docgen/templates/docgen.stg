>>

datatype(datatype) ::= <<
<!DOCTYPE html>
<html>
<body>
<h2>Datatype $datatype.name$</h2>
$documentation(object=datatype)$
<br>
$pedigree(object=datatype)$
<h3>Mappings</h3>
$mappings(datatype.mappedDatatype)$
</body>
</html>
>>

mappings(mappings) ::= <<
<table>
<tr>
<th>Standard</th>
<th>Base type</th>
<th>Parameter</th>
<th>Pattern</th>
</tr>
$mappings:{mapping|<tr><td>$mapping.standard$</td><td>$mapping.base$</td><td>$mapping.parameter$</td><td>$mapping.pattern$</td></tr>}$
</table>
>>

codeSet(codeSet) ::= <<
<!DOCTYPE html>
<html>
<body>
<h2>Code Set $codeSet.name$</h2>
$documentation(object=codeSet)$
<br>
<table>
<tr>
<th>ID</th>
<th>Datatype</th>
</tr>
<tr><td>$codeSet.id$</td><td><a href="$codeSet.type$.html">$codeSet.type$</a></td></tr>
</table>
