protocols(protocols) ::= <<
<!DOCTYPE html>
<html>
<body>
Protocol <select id="protocolSelect" onchange="protocolChange(this)" onload="protocolChange(this)">
    $protocols:{protocol |<option value="$protocol.name$">$protocol.name$</option>}$
 </select>
 <div id="message"></div>
</body>
</html>
>>

sections(sections) ::= <<
<!DOCTYPE html>
<html>
<body>
Section <select id="sectionSelect" onchange="sectionChange(this)" onload="sectionChange(this)">
    $sections:{section |<option value="$section.name$">$section.name$</option>}$
 </select>
 <div id="message"></div>
</body>
</html>

<script type="text/javascript">
function protocolChange(control) {
	document.getElementById("message").innerHTML = control.value;
}
</script>
>>

