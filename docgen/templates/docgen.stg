</body>
</html>
>>

codes(codes) ::= <<
<table>
<tr>
<th>ID</th>
<th>Name</th>
<th>Value</th>
<th>Description</th>
<th>Added</th>
<th>EP</th>
<th>Updated</th>
<th>EP</th>
<th>Deprecated</th>
<th>EP</th>
</tr>
$codes:{code|<tr><td>$code.id$</td><td>$code.name$</td><td>$code.value$</td><td>$documentation(object=code)$</td><td>$code.added$</td><td>$code.addedEP$</td><td>$code.updated$</td><td>$code.updatedEP$</td><td>$code.deprecated$</td><td>$code.deprecatedEP$</td></tr>}$
</table>
>>

pedigree(object) ::= <<
<table>
<tr>
<th>Added</th>
<th>EP</th>
<th>Updated</th>
<th>EP</th>
<th>Deprecated</th>
<th>EP</th>
</tr>
<tr><td>$object.added$</td><td>$object.addedEP$</td><td>$object.updated$</td><td>$object.updatedEP$</td><td>$object.deprecated$</td><td>$object.deprecatedEP$</td></tr>
</table>
>>

documentation(object) ::= <<
$object.annotation.DocumentationOrAppinfo:{doc|$doc.content$<br>}$
>>

